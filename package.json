{
	"name": "hmz-site",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "hexo clean && hexo generate && hexo server -p 4002",
		"publish": "npm run clean && npm run build && gulp",
		"search": "hexo algolia -n",
		"build": "hexo generate",
		"clean": "hexo clean",
		"deploy": "hexo deploy",
		"server": "hexo server",
